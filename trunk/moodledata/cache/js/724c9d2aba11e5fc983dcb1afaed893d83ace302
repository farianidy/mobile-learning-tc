selector={select:null,input:null,button:null,filter_init:function(strsearch,selectinputid){selector.select=document.getElementById(selectinputid);selector.button=document.getElementById('settingssubmit');var div=document.createElement('div');div.id='searchui';var input=document.createElement('input');input.type='text';input.id=selectinputid+'_search';selector.input=input;var label=document.createElement('label');label.htmlFor=input.id;label.appendChild(document.createTextNode(strsearch+' '));div.appendChild(label);div.appendChild(input);selector.select.parentNode.insertBefore(div,selector.select);YUI().use('yui2-event',function(Y){Y.YUI2.util.Event.addListener(input,'keyup',selector.filter_change)})},filter_change:function(){var searchtext=selector.input.value.toLowerCase(),options=selector.select.options,matchingoption=-1;for(var i=0;i<options.length;i++){var optiontext=options[i].text.toLowerCase();if(optiontext.indexOf(searchtext)>=0){options[i].disabled=false;options[i].style.display='block';if(matchingoption==-1)matchingoption=i}else{options[i].disabled=true;options[i].selected=false;options[i].style.display='none'}};if(matchingoption==-1){selector.input.className="error"}else selector.input.className=""}}